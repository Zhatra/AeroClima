{
    "builds": [
      {
        "src": "AeroClima/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src":"/static/(.*)",
        "dest":"/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "AeroClima/wsgi.py"
      }
    ]
}
